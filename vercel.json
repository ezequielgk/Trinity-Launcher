{
  "rewrites": [
    {
      "source": "/wiki/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/wiki",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/wiki/pages/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}